<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--自定义viewpager-->
    <declare-styleable name="GradientIconView">
        <attr name="top_icon" format="reference" />
        <attr name="bottom_icon" format="reference" />
    </declare-styleable>
    <!--自定义viewpager-->
    <declare-styleable name="GradientTextView">
        <attr name="text" format="string" />
        <attr name="text_size" format="dimension" />
        <attr name="top_text_color" format="color" />
        <attr name="bottom_text_color" format="color" />
    </declare-styleable>
    <!--广告栏-->
    <declare-styleable name="FlyBanner">
        <attr name="points_visibility" format="boolean" />
        <attr name="points_position" format="enum">
            <enum name="CENTER" value="0" />
            <enum name="LEFT" value="1" />
            <enum name="RIGHT" value="2" />
        </attr>
        <attr name="points_container_background" format="reference|color" />
    </declare-styleable>

</resources>